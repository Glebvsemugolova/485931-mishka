{
  "browserslist": [
    "last 2 versions",
    "IE 11",
    "Firefox ESR"
  ],
  "dependencies": {
    "gulp-htmlmin": "^4.0.0"
  },
  "description": "Личный проект «Мишка» от HTML Academy",
  "devDependencies": {
    "@htmlacademy/editorconfig-cli": "1.x",
    "autoprefixer": "7.2.x",
    "browser-sync": "2.23.x",
    "del": "3.0.0",
    "gulp": "3.9.x",
    "gulp-csso": "3.0.1",
    "gulp-htmlmin": "4.0.0",
    "gulp-imagemin": "4.1.0",
    "gulp-plumber": "1.2.x",
    "gulp-postcss": "7.0.x",
    "gulp-posthtml": "3.0.4",
    "gulp-rename": "1.2.2",
    "gulp-sass": "3.1.x",
    "gulp-svgstore": "6.1.1",
    "gulp-uglify": "3.0.0",
    "gulp-webp": "3.0.0",
    "posthtml-include": "1.1.0",
    "run-sequence": "2.2.1",
    "stylelint": "8.4.x",
    "stylelint-config-htmlacademy": "0.1.x"
  },
  "editorconfig-cli": [
    "*.json",
    "*.js",
    "source/*.html",
    "source/js/**/*.js",
    "source/img/**/*.svg",
    "source/sass/**/*.{sass,scss}"
  ],
  "engines": {
    "node": "8.9"
  },
  "private": true,
  "scripts": {
    "build": "gulp build",
    "editorconfig": "editorconfig-cli",
    "start": "npm run build && gulp serve",
    "stylelint": "stylelint 'source/sass/**/*.scss' --syntax scss",
    "test": "npm run editorconfig && npm run stylelint"
  },
  "version": "13.0.0"
}
